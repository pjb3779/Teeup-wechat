{
  "pages": [
    "pages/index/index",
    "pages/logs/logs",
    "pages/chatting/chatting",
    "pages/like/like",
    "pages/profile/profile",
    "pages/rate/rate"
  ],
  "window": {
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom"
  },
  "permission":{
      "scope.userLocation": {
          "desc": "현재 위치 표시"
      }
  },
  "usingComponents": {
    "navigation-bar": "/components/navigation-bar/navigation-bar",
    "van-tabbar": "@vant/weapp/tabbar/index",
    "van-tabbar-item": "@vant/weapp/tabbar-item/index",
    "van-icon": "@vant/weapp/icon/index",
    "van-button": "@vant/weapp/button/index",
    "van-config-provider": "@vant/weapp/config-provider/index",
    "van-rate":"@vant/weapp/rate/index",
    "van-dialog":"@vant/weapp/dialog/index",
    "van-cell": "@vant/weapp/cell/index",
    "van-popup": "@vant/weapp/popup/index",
    "van-calendar": "@vant/weapp/calendar/index",
    "van-dropdown-menu": "@vant/weapp/dropdown-menu/index",
    "van-dropdown-item": "@vant/weapp/dropdown-item/index",
    "van-search": "@vant/weapp/search/index"
  },
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "tagNameStyleIsolation": "legacy",
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  }, 
  "tabBar": {  
    "custom": true,
    "list": [
      { "pagePath": "pages/logs/logs", "text": "Home" },
      { "pagePath": "pages/chatting/chatting", "text": "Chat" },
      { "pagePath": "pages/like/like", "text": "Like" },
      { "pagePath": "pages/profile/profile", "text": "Profile" }
    ]
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents"
}